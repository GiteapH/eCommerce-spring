<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.ebusiness.mapper.UserActMapper">


    <select id="UserTypeCount" resultType="com.example.ebusiness.entity.typeCount">
        select u.act_type,count(act_type) as counts from user_act u where user =#{user} GROUP BY u.act_type
    </select>
</mapper>
